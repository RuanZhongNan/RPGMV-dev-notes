{
	"name": "@ruan-cat-docs/root",
	"private": true,
	"version": "1.0.0",
	"description": "笔记单仓根目录。",
	"license": "MIT",
	"type": "module",
	"engines": {
		"vscode": "^1.22.0",
		"node": ">=20",
		"pnpm": ">=9"
	},
	"packageManager": "pnpm@9.10.0",
	"scripts": {
		"preinstall": "npx only-allow pnpm && npm run corepack:pnpm",
		"corepack:pnpm": "corepack enable && corepack install",
		"test": "vitest --ui",
		"prettier": "prettier --write .",
		"git:dev-2-main": "git rebase dev main",
		"git:main-2-dev": "git rebase main dev",
		"update-package": "pnpm dlx vp-update",
		"turbo:update-packages": "turbo docs:update-package",
		"rm:node_modules": "pnpm -r rimraf node_modules && rimraf node_modules",
		"clear": "rimraf dist && rimraf .turbo && rimraf .vercel",
		"pnpm:clear": "pnpm -r clear && pnpm run clear",
		"turbo:build-docs": "turbo build:docs",
		"turbo:copy-dist": "turbo copy-dist",
		"on-github-action-flow": "turbo copy-dist",
		"deploy-vercel": "node --import=tsx ./bin/vercel-deploy-tool.ts"
	},
	"dependencies": {
		"@ruan-cat/utils": "^1.0.4",
		"@vercel/analytics": "^1.3.1",
		"@vueuse/core": "^10.11.1",
		"animate.css": "^4.1.1",
		"axios": "^1.7.7",
		"dayjs": "^1.11.13",
		"element-plus": "^2.8.3",
		"lodash-es": "^4.17.21",
		"node-localstorage": "^2.2.1",
		"pinia": "^2.2.2",
		"pinia-plugin-persistedstate": "^3.2.3",
		"uuid": "^9.0.1",
		"vue": "^3.5.6",
		"vue-json-excel3": "^0.0.9",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@commitlint/lint": "^19.5.0",
		"@commitlint/types": "^19.5.0",
		"@iconify/json": "^2.2.250",
		"@iconify/vue": "^4.1.2",
		"@ruan-cat/vercel-deploy-tool": "latest",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.16.5",
		"@types/uuid": "^9.0.8",
		"@vitejs/plugin-vue": "^4.6.2",
		"@vitest/ui": "^1.6.0",
		"@vuepress/bundler-vite": "2.0.0-rc.15",
		"@vuepress/plugin-docsearch": "2.0.0-rc.44",
		"@vuepress/plugin-register-components": "2.0.0-rc.44",
		"cpx": "^1.5.0",
		"cross-env": "^7.0.3",
		"cz-git": "^1.9.4",
		"mermaid": "^10.9.1",
		"mkdirp": "^3.0.1",
		"moment": "^2.30.1",
		"only-allow": "^1.2.1",
		"prettier": "^3.3.3",
		"prettier-plugin-lint-md": "^1.0.1",
		"rimraf": "^5.0.10",
		"shx": "^0.3.4",
		"tsx": "^4.19.1",
		"turbo": "^2.1.2",
		"typedoc": "^0.25.13",
		"typedoc-plugin-markdown": "^3.17.1",
		"typescript": "^5.6.2",
		"unplugin-auto-import": "^0.16.7",
		"unplugin-vue-components": "^0.25.2",
		"vercel": "34.1.2",
		"vite-node": "^1.6.0",
		"vitest": "^1.6.0",
		"vuepress": "2.0.0-rc.15",
		"vuepress-plugin-md-enhance": "2.0.0-rc.52",
		"vuepress-plugin-search-pro": "2.0.0-rc.52",
		"vuepress-plugin-typedoc": "0.13.0",
		"vuepress-theme-hope": "2.0.0-rc.52"
	}
}