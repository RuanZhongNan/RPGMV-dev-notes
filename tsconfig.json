{
	"compilerOptions": {
		"module": "ES2015",
		"target": "ES2015",
		"strict": true,
		"declaration": true,
		"composite": true,
		"emitDeclarationOnly": true,
		"moduleResolution": "Node",
		"typeRoots": [
			"./types"
		]
	},
	"include": [
		"./docs/.vuepress/components/drill-goods/**/*.ts",
		"docs/**/*.ts",
		"docs/**/*.d.ts",
		"docs/**/*.tsx",
		"docs/**/*.vue",
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.tsx",
		"src/**/*.vue",
	],
}